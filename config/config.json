{
  "$schema": "https://dev.office.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "rxslider-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/rxslider/RxsliderWebPart.js",
          "manifest": "./src/webparts/rxslider/RxsliderWebPart.manifest.json"
        }
      ]
    },
    "centralnews-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/centralnews/CentralnewsWebPart.js",
          "manifest": "./src/webparts/centralnews/CentralnewsWebPart.manifest.json"
        }
      ]
    },
    "events-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/events/EventsWebPart.js",
          "manifest": "./src/webparts/events/EventsWebPart.manifest.json"
        }
      ]
    },
    "tabsnews-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/tabsnews/TabsnewsWebPart.js",
          "manifest": "./src/webparts/tabsnews/TabsnewsWebPart.manifest.json"
        }
      ]
    },
    "menu-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/menu/MenuWebPart.js",
          "manifest": "./src/webparts/menu/MenuWebPart.manifest.json"
        }
      ]
    },
    "usersdirectory-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/usersdirectory/UsersdirectoryWebPart.js",
          "manifest": "./src/webparts/usersdirectory/UsersdirectoryWebPart.manifest.json"
        }
      ]
    },
    "todaysbirthdays-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/todaysbirthdays/TodaysbirthdaysWebPart.js",
          "manifest": "./src/webparts/todaysbirthdays/TodaysbirthdaysWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "SlideWebPartStrings": "lib/webparts/slide/loc/{locale}.js",
    "RxsliderWebPartStrings": "lib/webparts/rxslider/loc/{locale}.js",
    "CentralNewsWebPartStrings": "lib/webparts/centralNews/loc/{locale}.js",
    "EventosWebPartStrings": "lib/webparts/eventos/loc/{locale}.js",
    "CentralnewsWebPartStrings": "lib/webparts/centralnews/loc/{locale}.js",
    "EventsWebPartStrings": "lib/webparts/events/loc/{locale}.js",
    "TabsnewsWebPartStrings": "lib/webparts/tabsnews/loc/{locale}.js",
    "MenuWebPartStrings": "lib/webparts/menu/loc/{locale}.js",
    "UsersdirectoryWebPartStrings": "lib/webparts/usersdirectory/loc/{locale}.js",
    "TodaysbirthdaysWebPartStrings": "lib/webparts/todaysbirthdays/loc/{locale}.js"
  }
}
